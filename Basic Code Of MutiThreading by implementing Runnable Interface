import java.lang.*;
import java.io.*;
import java.util.*;

class Paras implements Runnable
{
	public void run() 
	{
		System.out.println("hello there!");
	}
}

public class UdemyClasses
{
	public static void main(String args[]) 
	{
		Paras obj = new Paras();
		Thread tobj = new Thread(obj);
		tobj.start();
	}
}
